def Rlisp
  to_execute = yield

  if to_execute.is_a?(Array)
    puts to_execute[1] if to_execute.first == :print
    return to_execute[1] + to_execute[2] if to_execute.first == :+
    return to_execute[1] - to_execute[2] if to_execute.first == :-
    return to_execute[1] * to_execute[2] if to_execute.first == :*
    return to_execute[1] / to_execute[2] if to_execute.first == :/
  elsif to_execute.nil?
    nil
  else
    fail 'Must be given an Array'
  end
end

class RlispExecutor

  SIMPLE_SEND

  OPERATIONS = {
    :+ =>
  }

  def initialize(execution_array)
    @execute_me = execution_array
  end

  def execute
  end
end
